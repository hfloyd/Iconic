{
  "propertyEditors": [
    {
      "alias": "our.iconic",
      "name": "Iconic",
      "icon": "icon-binoculars",
      "group": "Pickers",
      "editor": {
        "view": "~/app_plugins/Iconic/Views/iconic.html",
        "valueType": "JSON"
      },
      "prevalues": {
        "fields": [
          {
            "label": "Packages configuration",
